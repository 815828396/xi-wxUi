.xi-swiper-dots{bottom:20rpx;padding:10rpx 0;width:100%;display:flex;justify-content:center}.xi-swiper-dots .xi-circle{margin:0 5rpx}.xi-swiper-dots .dots{margin:0 10rpx;background:#C8C8D2}.xi-swiper-dots .dots.square{width:15rpx;height:8rpx;transform:skew(-20deg);transition:width .3s}.xi-swiper-dots .dots.square.active{width:30rpx;background:#1EA0FF}