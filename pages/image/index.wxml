<view class="xi-list">
  <view class="xi-list__header">上传图片,不可修改</view>
  <view style="width: 200rpx; height: 200rpx">

    <xi-image
      zoom
      cannotUpdate
      upload-url="{{ uploadUrl }}"
      privew-image-path="{{ img }}"
    >
    </xi-image>
  </view>
  <view style="height: 300rpx;margin-top: 30rpx">
    <view class="xi-list__header">上传图片，带进度条样式</view>
    <xi-image
      zoom
      upload-url="{{ uploadUrl }}"
      privew-image-path="{{ imgs }}"
    >
    </xi-image>
  </view>
</view>